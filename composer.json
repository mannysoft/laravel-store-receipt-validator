{
    "name": "mannysoft/laravel-store-receipt-validator",
    "description": "Laravel receipt validator for Apple iTunes, Google Play",
    "keywords": [
        "laravel",
        "validator",
        "receipt",
        "apple",
        "android",
        "in app purchase"
    ],
    "authors": [
        {
            "name": "Manny Isles"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-0": {
            "Mannysoft\\StoreReceiptValidator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mannysoft\\StoreReceiptValidator\\ServiceProvider"
            ],
            "aliases": {
                "StoreReceiptValidator": "Mannysoft\\StoreReceiptValidator\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
